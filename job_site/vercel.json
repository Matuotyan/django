{
    "regions": [
        "hnd1"
    ],
    "builds": [
        {
            "src": "job_site/asgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "static"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "job_site/asgi.py"
        },
        {
            "src": "/(.*)",
            "dest": "job_site/wsgi.py"
        }
    ]
}